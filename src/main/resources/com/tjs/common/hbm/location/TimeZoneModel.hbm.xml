<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tjs.common.location.model">
	<class entity-name="timezoneModel" name="TimeZoneModel" table="tbltimezone" dynamic-update="true" dynamic-insert="true">
		<id name="id" column="pkid" type="java.lang.Integer">
			<generator class="identity" />
		</id>
		<property name="timezone" column="txttimezone" type="java.lang.String" not-null="true" length="50" />
	</class>
</hibernate-mapping>